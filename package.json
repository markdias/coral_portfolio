{
  "name": "coral-portfolio-monorepo",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:admin": "npm --prefix admin run dev",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:admin\"",
    "install:all": "npm --prefix frontend install && npm --prefix admin install",
    "build": "npm --prefix frontend run build && npm --prefix admin run build"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}

