# Copy to .env and set your local admin password(s)

# Used in the browser app (embedded at build time). Local-only usage.
VITE_ADMIN_PASSWORD=coraldias

# Used by the local Admin API server to authorize publish requests.
ADMIN_PASSWORD=coraldias

# Optional: Customize ports/hosts to avoid conflicts
# Client (Vite) dev server
VITE_ADMIN_CLIENT_HOST=127.0.0.1
VITE_ADMIN_CLIENT_PORT=5174

# Local Admin API server
ADMIN_API_HOST=127.0.0.1
ADMIN_API_PORT=5175
